<!DOCTYPE html>
<html>
<head>
  <title>Patient Check-in Portal</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<h3>
			<a href="/">Home</a>
		</h3>
		<div class="row">
			<div class="span3 calendar-tab" data-spy="affix" data-offset-top="200">
				<%= render 'layouts/calendar' %>				
			</div>
			<div class="span3 pull-left"></div>
			<div class="span9">
				<%= yield %>
			</div>
		</div>
	</div>

</body>
</html>
